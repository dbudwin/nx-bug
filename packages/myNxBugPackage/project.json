{
  "name": "@dbudwin/test-package",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "{projectRoot}/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:copy",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist",
        "assets": [
          {
            "input": "src",
            "glob": "**/*",
            "output": "."
          }
        ]
      }
    },
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm install --filter {projectRoot}"
        ]
      }
    }
  }
}
